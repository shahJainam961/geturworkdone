<%- include("header") -%>

    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>

    <div class="box">
        <form class="item" action="/" method="post">
            <input type="text" autocomplete="off" name="newItem" placeholder="New Item">
            <button type="submit" class="add" name="list" value=<%= listTitle %>>+</button>
        </form>
        <% for(var i=0;i<newDo.length;i++){%>
            <div class="item">
                <input type="checkbox">
                <p><%= newDo[i] %></p>
            </div>  
        <%}%>
    </div>
<%- include("footer") -%>